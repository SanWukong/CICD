pipeline {
    agent any
        
    stages {
        stage('Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/SanWukong/CICD.git'
            }
        }
      stage('Deploy') {
            steps {
                sh 'kubectl get pods'
                sh 'ls -l && pwd'
                sh 'kubectl -f apply deployement.yaml'
            }
        }
    }
}
