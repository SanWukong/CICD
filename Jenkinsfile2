pipeline {
    agent any
        
    stages {
        stage('Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/SanWukong/CICD.git'
            }
        }
      stage('Deploy') {
            steps {
                sh 'alias k="kubectl"'
                sh 'ls -l'
                sh 'k get pods'
            }
        }
    }
}
